{
  "name": "sequence-arasaac",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-intl": "^6.2.10",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "test-coverage": "react-scripts test --coverage --watchAll=false  --passWithNoTests",
    "prepare": "npm run compile -- languages/en.json --ast --out-file src/languages/en.json && npm run compile -- languages/ca.json --ast --out-file src/languages/ca.json && npm run compile -- languages/es.json --ast --out-file src/languages/es.json",
    "extract": "formatjs extract",
    "compile": "formatjs compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^6.0.4",
    "axios": "^1.3.4",
    "husky": "^8.0.0",
    "msw": "^1.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts",
      "!src/setupTests.ts",
      "!src/**/*.test.tsx",
      "!src/app/store.ts"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
